<TriplesMap6>
    a rr:TriplesMap;

    rml:logicalSource[
        rml:source "w3.csv";
        rml:referenceFormulation ql:CSV
    ];

    rr:subjectMap [
        rr:template "http://example.org/{ID}"
    ];
    
    rr:predicateObjectMap[
        rr:predicate rdf:type;
        rr:objectMap [
           rr:parentTriplesMap <#TriplesMap7>;
           rr:joinCondition[
               rr:child "ID";
               rr:parent "w3.ID";
           ]
        ]
    ];.

<TriplesMap7>
    a rr:TriplesMap;
    
    rml:logicalSource[
        [rml:souce "w3.csv";]
        [rml:referenceFormulation ql:CSV]
        [
            rml:iterator "$.target[*]"
            ]
    ];
    
    rr:subjectMap[
        rr:template "http://example.org/{ID}"
    ].